<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/victimFragment">

    <fragment
        android:id="@+id/victimFragment"
        android:name="com.example.localstealcon.MainFragments.VictimFragment"
        android:label="fragment_victim"
        tools:layout="@layout/fragment_victim" >
        <action
            android:id="@+id/action_victimFragment_to_wordFragment"
            app:destination="@id/wordFragment" />
    </fragment>
    <fragment
        android:id="@+id/wordFragment"
        android:name="com.example.localstealcon.MainFragments.WordFragment"
        android:label="fragment_word"
        tools:layout="@layout/fragment_word" >
        <action
            android:id="@+id/action_wordFragment_to_victimFragment"
            app:destination="@id/victimFragment" />
    </fragment>
</navigation>
